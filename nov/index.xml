      <pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate>
      <guid>https://crll01.github.io/nov/radio/</guid>
      <description>&lt;style&gt;&#xA;  .lock-wrap{&#xA;    max-width:720px;margin:2.2rem auto;padding:1.2rem 1.2rem 1.4rem;&#xA;    border:1px solid rgba(0,0,0,.12);border-radius:14px;background:rgba(250,250,250,.75)&#xA;  }&#xA;  .lock-title{font-size:1.2rem;font-weight:800;margin:0 0 .5rem}&#xA;  .lock-hint{margin:0 0 1rem;opacity:.86;line-height:1.6}&#xA;  .lock-row{display:flex;gap:.6rem;flex-wrap:wrap;align-items:center}&#xA;  .lock-input{&#xA;    flex:1 1 220px;padding:.7rem .85rem;border-radius:10px;&#xA;    border:1px solid rgba(0,0,0,.18);font-size:1rem&#xA;  }&#xA;  .lock-btn{&#xA;    padding:.72rem .95rem;border-radius:10px;border:1px solid rgba(0,0,0,.18);&#xA;    cursor:pointer;font-weight:700;background:#fff&#xA;  }&#xA;  .lock-msg{margin-top:.85rem;font-size:.98rem;min-height:1.2em}&#xA;  .lock-msg.ok{color:#0a7a2f}.lock-msg.err{color:#b00020}&#xA;  #protected{display:none}&#xA;&lt;/style&gt;&#xA;&lt;div class=&#34;lock-wrap&#34; id=&#34;lockBox&#34;&gt;&#xA;  &lt;div class=&#34;lock-title&#34;&gt;White Orchard&lt;/div&gt;&#xA;  &lt;div class=&#34;lock-row&#34;&gt;&#xA;    &lt;input class=&#34;lock-input&#34; id=&#34;pw&#34; type=&#34;password&#34; placeholder=&#34;Password…&#34; /&gt;&#xA;    &lt;button class=&#34;lock-btn&#34; id=&#34;unlockBtn&#34; type=&#34;button&#34;&gt;Unlock&lt;/button&gt;&#xA;  &lt;/div&gt;&#xA;  &lt;div class=&#34;lock-msg&#34; id=&#34;msg&#34;&gt;&lt;/div&gt;&#xA;&lt;/div&gt;&#xA;&lt;div id=&#34;protected&#34;&gt;&#xA;  &lt;/p&gt;天老地荒 最好忘记&lt;/p&gt;&#xA;  &lt;/p&gt;笑也轻微 痛也轻微&lt;/p&gt;&#xA;  &lt;/p&gt;生老病死 相聚分离&lt;/p&gt;&#xA;  &lt;/p&gt;身不由己 心不由己&lt;/p&gt;&#xA;  &lt;/p&gt;浮萍落花 颠沛流离&lt;/p&gt;&#xA;  &lt;/p&gt;山盟海誓 力竭筋疲&lt;/p&gt;&#xA;  &lt;/p&gt;笑飲砒霜 魄散魂離&lt;/p&gt;&#xA;  &lt;/p&gt;愛有盡時 恨無絕期&lt;/p&gt;&#xA;&lt;/div&gt;&#xA;&lt;script&gt;&#xA;  const PASSWORD = &#34;password&#34;;&#xA;&#xA;  const lockBox = document.getElementById(&#34;lockBox&#34;);&#xA;  const protectedEl = document.getElementById(&#34;protected&#34;);&#xA;  const pw = document.getElementById(&#34;pw&#34;);&#xA;  const btn = document.getElementById(&#34;unlockBtn&#34;);&#xA;  const msg = document.getElementById(&#34;msg&#34;);&#xA;&#xA;  function setMsg(text, ok){&#xA;    msg.textContent = text;&#xA;    msg.className = &#34;lock-msg &#34; + (ok ? &#34;ok&#34; : &#34;err&#34;);&#xA;  }&#xA;&#xA;  function unlock(){&#xA;    if (pw.value === PASSWORD){&#xA;      lockBox.style.display = &#34;none&#34;;&#xA;      protectedEl.style.display = &#34;block&#34;;&#xA;    } else {&#xA;      setMsg(&#34;密码不对&#34;, false);&#xA;      pw.focus(); &#xA;      pw.select();&#xA;    }&#xA;  }&#xA;&#xA;  btn.addEventListener(&#34;click&#34;, unlock);&#xA;  pw.addEventListener(&#34;keydown&#34;, (e)=&gt;{&#xA;    if (e.key === &#34;Enter&#34;) unlock();&#xA;  });&#xA;&lt;/script&gt;</description>
